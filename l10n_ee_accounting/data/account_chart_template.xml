<odoo>
    <record id="chart_template" model="account.chart.template">
        <field name="name">Estonian Chart of Accounts Template (OCA)</field>
        <field name="currency_id" ref="base.EUR" />
        <field name="country_id" ref="base.ee" />
        <field name="code_digits">6</field>
        <field name="complete_tax_set" eval="True" />
        <field name="use_anglo_saxon" eval="False" />
        <field name="transfer_account_code_prefix">1010</field>
        <field name="bank_account_code_prefix">1012</field>
        <field name="cash_account_code_prefix">1011</field>
    </record>
</odoo>
